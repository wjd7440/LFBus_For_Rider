{
  "expo": {
    "name": "Rider",
    "slug": "Rider",
    "version": "1.0.0",
    "orientation": "default", // landscape 가로모드, portrait 세로 모드, default 가로/세로
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.wezoncoop.rider",
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}


